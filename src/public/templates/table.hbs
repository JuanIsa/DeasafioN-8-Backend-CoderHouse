<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Producto</th>
            <th scope="col">Precio</th>
            <th scope="col">Foto</th>
        </tr>
    </thead>
    <tbody>
        {{#each productos}}
        <tr>
            <th scope="row">{{this.id}}</th>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img src={{this.thumbnail}} alt="" class="fotos"></td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{#if renderConditional}}
<h3>No hay productos en la lista.</h3>
{{/if}}